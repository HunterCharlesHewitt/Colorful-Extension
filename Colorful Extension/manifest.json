{

"manifest_version":2,
"minimum_chrome_version":"45",
"author":"Hunter Hewitt",
"homepage_url":"https://github.com",
"name":"Color Filter",
"description":"Colorful Chrome image filter inspired by Steven Roberts' Crazy Pages",
"version":"1.0",
"icons":{"16":"icon-16.png","48":"icon-48.png","128":"icon-128.png"},
"browser_action":{"default_icon":{"48":"icon-48.png"}},
"web_accessible_resources":["animations.css"],
"commands":{"toggle-animations":{"suggested_key":{"default":"Alt+C"},
"description":"Toggle Colorful Filter"}},
"offline_enabled":true,
"permissions":["activeTab"],
"background":{"persistent":false,
"scripts":["background.js"]}
}
